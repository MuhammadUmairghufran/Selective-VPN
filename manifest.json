{
  "name": "Selective VPN",
  "version": "0.5",
  "author": "Vadagon",
  "description": "Use VPN connection on specific websites",
  "browser_action": {
    "default_icon": "img/spears.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "img/*",
    "assets/*"
  ],
  "icons": {
    "256": "img/spears.png"
  },
  "permissions": [
    "proxy",
    "tabs",
    "webRequest",
    "storage",
    "http://*/", "https://*/", "<all_urls>"
  ],
  "manifest_version": 2
}
